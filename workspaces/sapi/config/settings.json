{
    "id": "servicenow-worker",
    "name": "Servicenow Worker",
    "debug": true,
    "release": {
        "id": "1",
        "name": "v0.0.1"
    },
    "integrations": {
        "sql_database": {
            "sapi": {
                "sgbd": "mysql+pymysql",
                "host": "${os.env['SAPI_HOST']}",
                "port": "${os.env['SAPI_PORT']}",
                "user": "${os.env['SAPI_USER']}",
                "password": "${os.env['SAPI_PASSWORD']}",
                "database": "${os.env['SAPI_DATABASE']}"
            }
        },
        "mongodb": {
            "socportal": {
                "host": "${os.env['MONGODB_HOST']}",
                "port": "${os.env['MONGODB_PORT']}",
                "user": "${os.env['MONGODB_USER']}",
                "password": "${os.env['MONGODB_PASSWORD']}",
                "database": "${os.env['MONGODB_DATABASE']}"
            }
        }
    },
    "env": {},
    "safe_mode": {
        "active": true,
        "safe_time": 10
    }
}


